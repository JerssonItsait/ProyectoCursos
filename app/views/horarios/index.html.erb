<%- model_class = Horario -%>
<div class="page-header">
  <h1>Horarios</h1>
</div>
<table class="table table-striped table-bordered table-condensed">
  <thead>
    <tr>
      <th>id</th>
      <th>dia</th>
      <th>hora</th>
      <th>aula</th>
      <th>materia_id</th>
      <th>instructor_id</th>
      <th>curso_id</th> 
      <th>Acciones</th>     
      <th></th>
    </tr>
  </thead>
  <tbody>
     <%= render @horarios %>
  </tbody>
  <tfoot>
    <tr>
      <td><%= link_to 'Nuevos Horarios', new_horario_path, :class => 'btn btn-success', :remote => true %></td>
    </tr>
  </tfoot>
</table>
