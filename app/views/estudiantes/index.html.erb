<%- model_class = Estudiante -%>
<div class="page-header">
  <h1>Estudiantes</h1>
</div>
<table class="table table-striped table-bordered table-condensed">
  <thead>
    <tr>
      <th>id</th>
      <th>nombre</th>
      <th>cedula</th>
      <th>email</th>
      <th>direccion</th>
      <th>fch_nacimiento</th>
      <th>curso_id</th>
      <th>acciones</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
     <%= render @estudiantes %>
  </tbody>
  <tfoot>
    <tr>
      <td><%= link_to 'Nuevo Estudiante', new_estudiante_path, :class => 'btn btn-success', :remote => true %></td>
    </tr>
  </tfoot>
</table>
